config_version=5

[application]

config/name="I Am Using Force"
run/main_scene="res://Levels/Test.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://Sock.png"

[audio]

buses/default_bus_layout="res://Audio/default_bus_layout.tres"

[autoload]

Globals="*res://Singletons/Globals.gd"
GameHandler="*res://Singletons/Game.tscn"
DebugLibrary="*res://Singletons/DebugLibrary.tscn"
HelperLibrary="*res://Singletons/HelperLibrary.tscn"
BulletContainer="*res://Singletons/BulletContainer.tscn"
SignalBus="*res://Singletons/SignalBus.gd"
DebugMenu="*res://Singletons/DebugMenu.tscn"
Shaker="*res://addons/shaker/src/Shaker.gd"

[debug]

shader_language/warnings/unused_local_variable=false

[dotnet]

project/assembly_name="I Am Using Force"

[editor]

naming/scene_name_casing=1

[editor_plugins]

enabled=PackedStringArray("res://addons/ridiculous_coding/plugin.cfg", "res://addons/script-ide/plugin.cfg", "res://addons/shaker/plugin.cfg")

[file_customization]

folder_colors={
"res://Gadgets/": "gray",
"res://Weapons/": "red"
}

